{
  "name": "axis",
  "version": "0.1.9",
  "description": "A minimalistic, customizable, and beautiful browser built on Electron",
  "main": "src/main.js",
  "scripts": {
    "start": "npx electron .",
    "dev": "npx electron . --dev",
    "build": "npx electron-builder",
    "dist": "npx electron-builder --publish=never"
  },
  "overrides": {
    "tar": "^7.5.8",
    "ajv": "^8.18.0"
  },
  "keywords": [
    "browser",
    "electron",
    "minimalistic",
    "customizable"
  ],
  "author": "Axis Browser Team",
  "license": "SEE LICENSE IN LICENSE.md",
  "devDependencies": {
    "electron": "^33.4.11",
    "electron-builder": "^26.4.0",
    "lodash": "^4.17.23",
    "tar": "^7.5.8"
  },
  "dependencies": {
    "electron-store": "^8.1.0"
  },
  "build": {
    "appId": "com.axis.browser",
    "productName": "Axis",
    "directories": {
      "output": "dist"
    },
    "files": [
      "src/**/*",
      "assets/**/*",
      "node_modules/**/*"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
